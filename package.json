{
  "name": "vue3-mobile",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "build": "npm run type-check && vite build",
    "build:test": "npm run type-check && vite build --mode test",
    "build:pre": "npm run type-check && vite build --mode pre",
    "build:prod": "npm run type-check && vite build --mode prod",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "run-p lint:prettier lint:eslint lint:stylelint",
    "lint:prettier": "prettier . --write --log-level warn",
    "lint:eslint": "eslint . --fix",
    "lint:stylelint": "stylelint \"./src/**/*.{vue,scss,css}\" --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "pinia": "^2.2.4",
    "qs": "^6.13.0",
    "vue": "^3.5.11",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.10",
    "@types/qs": "^6.9.16",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-legacy": "^5.4.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^14.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "postcss-html": "^1.7.0",
    "postcss-preset-env": "^10.0.6",
    "prettier": "3.3.3",
    "sass-embedded": "^1.79.4",
    "stylelint": "^16.9.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "^5.5.4",
    "unplugin-auto-import": "^0.18.3",
    "vite": "^5.4.8",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.1.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,css}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.16.0",
    "pnpm": ">=9.0.0"
  }
}
