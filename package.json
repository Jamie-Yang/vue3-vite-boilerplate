{
  "name": "vue3-vite-boilerplate",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "build": "npm run type-check && vite build",
    "build:test": "npm run type-check && vite build --mode test",
    "build:pre": "npm run type-check && vite build --mode pre",
    "build:prod": "npm run type-check && vite build --mode prod",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "lint": "run-p lint:prettier lint:eslint lint:stylelint",
    "lint:eslint": "eslint . --fix",
    "lint:prettier": "prettier . --write --log-level warn",
    "lint:stylelint": "stylelint \"./src/**/*.{vue,scss,css}\" --fix",
    "eslint-inspect": "eslint --inspect-config",
    "reinstall": "pnpm clean --del-lock && pnpm install",
    "prepare": "is-ci || husky",
    "update:deps": "npx taze -i"
  },
  "dependencies": {
    "@iconify/vue": "^5.0.0",
    "axios": "^1.11.0",
    "pinia": "^3.0.3",
    "qs": "^6.14.0",
    "vue": "^3.5.19",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.33.0",
    "@types/node": "^22.17.2",
    "@types/qs": "^6.14.0",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^16.1.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-preset-env": "^10.2.4",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.90.0",
    "stylelint": "^16.23.1",
    "stylelint-config-recess-order": "^7.2.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^20.0.0",
    "vite": "^7.1.3",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^3.0.6"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js",
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "unrs-resolver"
    ]
  }
}
