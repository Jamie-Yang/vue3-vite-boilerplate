{
  "name": "vue3-mobile",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "build": "npm run type-check && vite build",
    "build:test": "npm run type-check && vite build --mode test",
    "build:pre": "npm run type-check && vite build --mode pre",
    "build:prod": "npm run type-check && vite build --mode prod",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "lint": "run-p lint:prettier lint:eslint lint:stylelint",
    "lint:prettier": "prettier . --write --log-level warn",
    "lint:eslint": "eslint . --fix",
    "lint:stylelint": "stylelint \"./src/**/*.{vue,scss,css}\" --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "pinia": "^2.3.0",
    "qs": "^6.13.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.10.2",
    "@types/qs": "^6.9.17",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "@vitejs/plugin-legacy": "^6.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-typescript": "^14.2.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "postcss-html": "^1.7.0",
    "postcss-preset-env": "^10.1.2",
    "prettier": "^3.4.2",
    "sass-embedded": "^1.83.0",
    "stylelint": "^16.12.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "^5.7.2",
    "unplugin-auto-import": "^0.19.0",
    "vite": "^6.0.6",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,css}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.16.0",
    "pnpm": ">=9.0.0"
  }
}
